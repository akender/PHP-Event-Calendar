<?php

class iCalendarEventParser
{
	var $events = false;
	
	function setParamsToArray($file)
	{
		$f = fopen($file, 'r');
		$this->events[] = false;
		$paramarray = false;
		$subparam[2] = false;
		while ($line = fgets($f))
		{
			$a = explode(':', $line, 2);
			array_push($subparam, $a[0], $a[1]);
			array_push($paramarray, $subparam);
			
			print_r($line.'<br>');
			explode(':', $line);
			array_push($paramarray, explode(':', $line)[0]);
		}
		fclose($f);
	}
	
	function test()
	{
		
	}
}

?>
