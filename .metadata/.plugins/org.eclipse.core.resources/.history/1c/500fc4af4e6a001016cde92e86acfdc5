<?php

class iCalendarEventParser
{
	var $events = false;
	var $paramarray = false;
	
	function setParamsToArray($file)
	{
		$f = fopen($file, 'r');
		$this->events[] = false;
		$this->paramarray[] = false;
		$subparam[2] = false;
		while ($line = fgets($f))
		{
			$a = explode(':', $line, 2);
			array_push($paramarray, $a[0]);
			
			print_r($line.'<br>');
			explode(':', $line);
			array_push($paramarray, explode(':', $line)[0]);
		}
		fclose($f);
	}
	
	function test()
	{
		
	}
}

?>
